import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner input =new Scanner(System.in);
	    
		System.out.println("Even numbers:");
		for(int i=1;i<=10;i++){
		    if(i%2==0){
		        
		        System.out.print(i);
		    }
		}
	    System.out.println();
	    System.out.println("Odd numbers:");
	    for(int i=1;i<=10;i++){
	        if(i%2!=0){
	            
	            System.out.print(i);
	    }
	}
