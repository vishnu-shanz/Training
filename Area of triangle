import java.util.*;
class sam2{
  private int a=0;
  private int b=0;
int Tri(int a, int b)
{
    int area=(a*b)/2;
    return area;
} 
}
class Main{
     public static void main(String[]args){
         System.out.println("Enter base & height");
         Scanner input=new Scanner(System.in);
         int n=input.nextInt();
         int m=input.nextInt();
        sam2 s=new sam2();
        int a=s.Tri(n,m);
        System.out.println(a);
        
    }
}
